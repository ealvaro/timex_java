<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="timex" match-name="departments"/>
  <table-filter match-catalog="timex" match-name="employees"/>
  <table-filter match-catalog="timex" match-name="payments"/>
  <table-filter match-catalog="timex" match-name="timesheets"/>
  
  <table catalog="timex" name="departments" class="Department" /> 
  <table catalog="timex" name="employees" class="Employee" /> 
  <table catalog="timex" name="payments" class="Payment" /> 
  <table catalog="timex" name="timesheets" class="Timesheet" /> 
  
</hibernate-reverse-engineering>